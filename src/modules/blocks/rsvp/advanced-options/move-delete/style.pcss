.tribe-editor__rsvp__content-row--move-delete {
	padding: 30px 0 0;

	& > button {
		background: transparent;
		border-width: 0;
		cursor: pointer;
		font-size: 15px !important;
		letter-spacing: .38px;
		line-height: 18px;
		outline: none;
		padding: 0;

		&:first-child {
			color: #009fd4;

			&::after {
				color: #8d949b;
				content: '|';
				margin: 0 10px;
			}
		}

		&:last-child {
			color: red;
		}
	}
}
